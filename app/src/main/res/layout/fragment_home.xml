<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#e5e5e5"
    tools:context="com.example.dontknow.techumen.home">

    <!-- TODO: Update blank fragment layout -->
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Code Wizard Novice"
            android:id="@+id/idgoc_novice"
            android:textAlignment="center"
            android:layout_margin="10dp"
            android:padding="10dp"
            android:textSize="20dp"
            android:textStyle="bold"
            android:onClick="onClick"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Code Wizard Expert"
            android:id="@+id/idgoc_expert"
            android:layout_below="@+id/idgoc_novice"
            android:textAlignment="center"
            android:layout_margin="10dp"
            android:padding="10dp"
            android:textSize="20dp"
            android:textStyle="bold"
            android:onClick="onClick"
            />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="ResPublica"
            android:id="@+id/idRepublica"
            android:layout_below="@+id/idgoc_expert"
            android:textAlignment="center"
            android:layout_margin="10dp"
            android:textSize="20dp"
            android:padding="10dp"
            android:textStyle="bold"
            android:onClick="onClick"/>
        <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
            android:hint="Mysterio_3_0"
            android:id="@+id/idmysterio"
            android:layout_below="@+id/idRepublica"
            android:textAlignment="center"
            android:layout_margin="10dp"
            android:padding="10dp"
            android:textSize="20dp"
            android:textStyle="bold"
            android:onClick="onClick"/>
        <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
            android:hint="WebON"
            android:id="@+id/idworkshop"
            android:layout_below="@+id/idmysterio"
            android:textAlignment="center"
            android:layout_margin="10dp"
            android:padding="10dp"
            android:textSize="20dp"
            android:textStyle="bold"
            android:onClick="onClick"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:hint="Spot games"
            android:id="@+id/idspot"
            android:layout_below="@+id/idworkshop"
            android:textAlignment="center"
            android:layout_margin="10dp"
            android:padding="10dp"
            android:textSize="20dp"
            android:textStyle="bold"
            android:onClick="onClick"/>
    </RelativeLayout>

</FrameLayout>
