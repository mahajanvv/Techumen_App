<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#e5e5e5"
    tools:context="com.example.dontknow.techumen.online">

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="CodeWizard_Novice"
            android:id="@+id/idonlinegoc_novice"
            android:textAlignment="center"
            android:layout_margin="10dp"
            android:padding="10dp"
            android:textSize="20dp"
            android:textStyle="bold"
            android:onClick="onClickinfo"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="CodeWizard_Expert"
            android:id="@+id/idonlinegoc_expert"
            android:layout_below="@+id/idonlinegoc_novice"
            android:textAlignment="center"
            android:layout_margin="10dp"
            android:padding="10dp"
            android:textSize="20dp"
            android:textStyle="bold"
            android:onClick="onClickinfo"
            />
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="ResPublica"
            android:id="@+id/idonlineRepublica"
            android:layout_below="@+id/idonlinegoc_expert"
            android:textAlignment="center"
            android:layout_margin="10dp"
            android:padding="10dp"
            android:textSize="20dp"
            android:textStyle="bold"
            android:onClick="onClickinfo"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="MYSTERIO_3_0"
            android:id="@+id/idonlinemysterio"
            android:layout_below="@+id/idonlineRepublica"
            android:textAlignment="center"
            android:layout_margin="10dp"
            android:padding="10dp"
            android:textSize="20dp"
            android:textStyle="bold"
            android:onClick="onClickinfo"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Web_ON"
            android:id="@+id/idonlineworkshop"
            android:layout_below="@+id/idonlinemysterio"
            android:textAlignment="center"
            android:layout_margin="10dp"
            android:padding="10dp"
            android:textSize="20dp"
            android:textStyle="bold"
            android:onClick="onClickinfo"/>
        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Spot Games"
            android:id="@+id/idonlinespot"
            android:layout_below="@+id/idonlineworkshop"
            android:textAlignment="center"
            android:layout_margin="10dp"
            android:padding="10dp"
            android:textSize="20dp"
            android:textStyle="bold"
            android:onClick="onClickinfo"/>
    </RelativeLayout>

</FrameLayout>
